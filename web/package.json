{
  "name": "online-gacha-backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "migrate": "node-pg-migrate -f database.json -m ./migrations --create-schema --migrations-table migrations",
    "seed": "node-pg-migrate -f database.json seed -m ./seeds"
  },
  "dependencies": {
    "fastify": "^4.27.2",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "node-pg-migrate": "^7.7.0"
  }
}
